@inherits CompositeC1WebPage
<!DOCTYPE html PUBLIC "-//W3C//DTD XHTML 1.0 Transitional//EN" "http://www.w3.org/TR/xhtml1/DTD/xhtml1-transitional.dtd">
<html xmlns="http://www.w3.org/1999/xhtml" xml:lang="en" lang="en">
<head>
    <title>@CurrentPageNode.Title</title>
    <meta name="author" content="superock" />
    <link rel="stylesheet" type="text/css" href="/Wedding/style.css" />
    <link rel="Stylesheet" type="text/css" href="/Wedding/js/big.slider/css/smoothDivScroll.css" />

    @if (!string.IsNullOrEmpty(CurrentPageNode.Description))
    {

        <meta name="Description" content="@CurrentPageNode.Description"/>
    }
    <f:function xmlns:f="http://www.composite.net/ns/function/1.0" name="Composite.Web.Html.Template.CommonMetaTags">
        <f:param name="Designer" value="Composite C1 starter site" />
    </f:function>
</head>
<body>
    @RenderBody()

    <script type="text/javascript" src="http://code.jquery.com/jquery-1.9.1.min.js"></script>
    <script src="http://ajax.googleapis.com/ajax/libs/jqueryui/1.8.18/jquery-ui.min.js"
        type="text/javascript"></script>
    <script type="text/javascript" src="/Wedding/js/jquery.easing.1.3.js"></script>
    <script type="text/javascript" src="/Wedding/js/big.slider/js/jquery.mousewheel.min.js"></script>
    <script type="text/javascript" src="/Wedding/js/big.slider/js/jquery.kinetic.js"></script>
    <script type="text/javascript" src="/Wedding/js/big.slider/js/jquery.smoothdivscroll-1.3-min.js"></script>
    <script type="text/javascript" src="/Wedding/js/wedding.js"></script>			
</body>
</html>